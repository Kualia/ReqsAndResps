program ReqRep;

uses
  Vcl.Forms,
  Main in '..\Main.pas' {MainForm},
  DataTab in '..\DataTab.pas',
  DBConnectionForm in '..\DBConnectionForm.pas' {Form1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
